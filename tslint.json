{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react",
        "tslint-config-airbnb"
    ],
    "jsRules": {},
    "rules": {
        "import-name": false,
        "member-access": false,
        "interface-over-type-literal": false,
        "indent": [true, "spaces", 2],
        "variable-name": [true, "allow-pascal-case"]
    },
    "rulesDirectory": []
}